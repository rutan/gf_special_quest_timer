{
  "manifest_version": 2,
  "name": "グラブル時限クエスト通知タイマー",
  "description": "グランブルーファンタジーの時限クエスト時刻を通知します（非公式）",
  "version": "0.0.1",
  "background": {
    "persistent": false,
    "scripts": [
      "js/zepto.min.js",
      "js/common.js",
      "js/notifier.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icons/512.png",
    "default_popup": "options.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "alarms",
    "management",
    "notifications",
    "http://gf.rutan.info/*"
  ]
}
